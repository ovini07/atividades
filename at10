package repeticao;

import java.util.Scanner;

public class atividade10 {
	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);
		int a = 0;    
		for (int i = 1; i <= 20; i++) {
			System.out.print("Informe o número (" + i + "/20): ");
			int n = ler.nextInt();
			if (n > 8) {
				a++;
			}
		}       
		System.out.println("Quantidade de números maiores que 8: " + a);       
		ler.close();
	}
}
